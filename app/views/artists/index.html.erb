<% @artists.each do |artist| %>
<div class="row">
  <div class="col-xs-6 col-md-3">
    <div class="thumbnail">
        <%= image_tag artist.image_url %>
        <h3><%= link_to artist.name, artist_path(artist) %></h3>
        <span class="label label-success"><%= artist.genre %></span>
    </div>
  </div>
</div>
<%end%>
<%= link_to "Add Artist", new_artist_path, class: "btn btn-primary" %>
